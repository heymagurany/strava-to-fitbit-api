{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "web root": {
      "matchCondition": {
        "methods": [
          "GET"
        ],
        "route": "/"
      },
      "backendUri": "https://stravatofitbit.blob.core.windows.net/web/index.html"
    },
    "web assets": {
      "matchCondition": {
        "methods": [
          "GET"
        ],
        "route": "/assets/{*file}"
      },
      "backendUri": "https://stravatofitbit.blob.core.windows.net/web/assets/{file}"
    },
    "web static": {
      "matchCondition": {
        "methods": [
          "GET"
        ],
        "route": "/{file}.{extension}"
      },
      "backendUri": "https://stravatofitbit.blob.core.windows.net/web/{file}.{extension}"
    },
    "OAuth callback": {
      "matchCondition": {
        "methods": [
          "GET"
        ],
        "route": "/oauth/callback/{serviceName}"
      },
      "backendUri": "https://strava-to-fitbit.azurewebsites.net/api/oauth-callback",
      "requestOverrides": {
        "backend.request.querystring.service_name": "{serviceName}"
      }
    }
  }
}